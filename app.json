{
  "name": "Hi",
  "description": "将v2ray部署在Heroku上",
  "keywords": [
    "v2ray VMess VLESS"
  ],
  "env": {
    "PROTOCOL": {
      "description": "协议填小写vless或vmess",
      "value": "vless",
      "required": true
    },
    "UUID": {
      "description": "用户的主ID",
      "value": "",
      "required": false
    },
    "WS_PATH": {
      "description": "websocket路径",
      "value": "/ray",
      "required": true
    }
  },
  "website": "https://github.com/iPad5588/xrayku",
  "repository": "https://github.com/iPad5588/xrayku",
  "stack": "container",
  "features": [
    "runtime-dyno-metadata"
  ]
}
